# Telegram Bot Token (get from @BotFather)
BOT_TOKEN=your_bot_token_here

# Payme Provider Token (get from Payme or @BotFather payments)
PAYMENT_PROVIDER_TOKEN=your_payme_provider_token_here

# Database URL (SQLite for development, PostgreSQL for production)
# SQLite: sqlite+aiosqlite:///./olympiad.db
# PostgreSQL: postgresql+asyncpg://user:password@localhost:5432/olympiad
DATABASE_URL=sqlite+aiosqlite:///./olympiad.db

# Admin Telegram IDs (comma-separated)
ADMIN_IDS=123456789,987654321

# Olympiad Price in tiyin (50000 = 500.00 UZS)
OLYMPIAD_PRICE=50000

# Currency code
OLYMPIAD_CURRENCY=UZS
